{
  "buildCommand": "bash build.sh",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/web/static/$1"
    },
    {
      "source": "/favicon.png",
      "destination": "/public/favicon.png"
    },
    {
      "source": "/favicon.ico",
      "destination": "/public/favicon.png"
    },
    {
      "source": "/robots.txt",
      "destination": "/public/robots.txt"
    },
    {
      "source": "/((?!web/|public/).*)",
      "destination": "/api/index"
    }
  ]
}
